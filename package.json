{
  "name": "webapp",
  "version": "2.1.1",
  "scripts": {
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/webapp/stats.json",
    "build": "ng build",
    "build:prod": "NODE_ENV=production ng build --prod",
    "format": "prettier --config .prettierrc --write \"src/**/*.{js,ts,json,md,css,html}\"",
    "generate-icons": "svg-to-ts",
    "lint": "eslint --color --fix -c .eslintrc.js --ext .ts ./src/app",
    "ng": "ng",
    "ng-update": "ng update --force @angular/cli@11 && ng update --force @angular/core@11 && ng update --force @angular/material@11",
    "start": "ng serve",
    "sonar": "sonar-scanner",
    "test": "ng test --code-coverage --watch=true --browsers=Chrome",
    "test:ci": "ng test --code-coverage --watch=false --browsers=ChromeHeadless"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "dependencies": {
    "@angular-material-components/datetime-picker": "^4.0.7",
    "@angular/animations": "12.2.16",
    "@angular/cdk": "~11.2.13",
    "@angular/common": "12.2.16",
    "@angular/compiler": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/forms": "12.2.16",
    "@angular/material": "^10.2.4",
    "@angular/material-moment-adapter": "^10.2.4",
    "@angular/platform-browser": "12.2.16",
    "@angular/platform-browser-dynamic": "12.2.16",
    "@angular/router": "12.2.16",
    "@auth0/angular-jwt": "^5.0.2",
    "@deck.gl/core": "8.4.16",
    "@deck.gl/layers": "8.4.16",
    "@deck.gl/mapbox": "8.4.16",
    "@types/file-saver": "2.0.2",
    "@types/lodash-es": "^4.17.4",
    "autoprefixer": "10.2.6",
    "country-code-lookup": "^0.0.19",
    "d3": "^7.1.0",
    "file-saver": "^2.0.5",
    "karma-sonarqube-unit-reporter": "^0.0.23",
    "keycloak-angular": "8.2.0",
    "keycloak-js": "^10.0.2",
    "lodash-es": "^4.17.21",
    "luxon": "1.27.0",
    "mapbox-gl": "^1.13.1",
    "moment": "^2.29.1",
    "ngx-clipboard": "~12.2.0",
    "ngx-daterangepicker-material": "^2.4.2",
    "ngx-mapbox-gl": "^4.8.1",
    "postcss": "8.3.0",
    "puppeteer": "^10.2.0",
    "rxjs": "6.6.7",
    "tailwindcss": "2.1.4",
    "tslib": "2.2.0",
    "uuid": "^8.3.2",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "4.4.2",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular-eslint/eslint-plugin": "^1.2.0",
    "@angular/cli": "^10.2.3",
    "@angular/compiler-cli": "12.2.16",
    "@angular/language-service": "12.2.16",
    "@ngneat/svg-icon": "^3.3.0",
    "@types/d3": "^7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "2.0.9",
    "@types/luxon": "^2.3.1",
    "@types/mapbox-gl": "2.1.2",
    "@types/node": "12.20.14",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "32.3.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^4.3.8",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "1.6.0",
    "karma-sonarqube-unit-reporter": "^0.0.23",
    "postcss-import": "14.0.2",
    "postcss-loader": "^4.3.0",
    "postcss-scss": "^3.0.4",
    "sonar-scanner": "^3.1.0",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "^3.0.0",
    "svg-to-ts": "^5.0.0",
    "ts-node": "~9.0.0",
    "typescript": "~4.3.5"
  },
  "svg-to-ts": {
    "generateType": "false",
    "delimiter": "KEBAB",
    "conversionType": "files",
    "iconsFolderName": "svg",
    "prefix": "app",
    "srcFiles": [
      "./src/assets/svg/*.svg"
    ],
    "outputDirectory": "./src/app",
    "svgoConfig": {
      "plugins": [
        {
          "removeDimensions": true,
          "cleanupAttrs": true
        }
      ]
    }
  }
}
